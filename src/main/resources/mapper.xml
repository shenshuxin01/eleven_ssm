<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ssx.dao.IDaoL">

    <resultMap id="rm" type="ssx.entity.POJOL">
        <id property="userId" column="user_id" />
        <result property="userName" column="user_name" />
    </resultMap>

    <select id="selectAll" resultMap="rm" >
        SELECT * FROM `demo`.`eleven_table` LIMIT 0,1000
    </select>


    <insert id="addOne" keyProperty="">
        insert into `demo`.`eleven_table`(user_name) values(#{name})
    </insert>
</mapper>